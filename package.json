  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "node-sass": "^9.0.0",
    "sass-loader": "^13.3.2",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11"
