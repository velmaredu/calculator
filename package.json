  "homepage": "https://velmaredu.github.io/calculator",
  "dependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "eslint-config-react-app": "^7.0.1",
    "gh-pages": "^6.0.0",
    "mathjs": "11.11.2",
